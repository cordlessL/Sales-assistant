# Telegram Bot Token
# Получите токен у @BotFather в Telegram
TELEGRAM_BOT_TOKEN=ваш_токен_бота_здесь

# GigaChat Authorization
# Вариант 1 (рекомендуется): Готовый Authorization key (Base64)
# Получите готовый Base64 ключ в личном кабинете GigaChat
GIGACHAT_AUTHORIZATION_KEY=ваш_ключ_авторизации_здесь

# Вариант 2: Отдельные client_id и client_secret
# Если нет готового ключа, используйте этот вариант
# Код автоматически закодирует их в Base64 (client_id:client_secret)
# GIGACHAT_CLIENT_ID=ваш_client_id_здесь
# GIGACHAT_CLIENT_SECRET=ваш_client_secret_здесь

# ProxyAPI Key для генерации изображений
# Получите ключ на https://proxyapi.ru
PROXY_API=ваш_proxy_api_ключ_здесь
